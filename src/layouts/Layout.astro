---
import Base from './Base.astro'
interface Props {
  title: string;
  description: string;
  image?: string;
  canonicalUrl?: string;
  type?: string;
  publishedTime?: string;
}

const { title, description } = Astro.props;
---

<Base 
  title={title}
  description={description}
  >
  <slot />
</Base>
